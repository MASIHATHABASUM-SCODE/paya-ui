<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fitStart"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgound"
    android:orientation="vertical"

    tools:context=".AgentActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <androidx.appcompat.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="@color/yellow"
            app:title="AgentPage" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingStart="15dp"
            android:paddingEnd="15dp">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="@string/cm"
                android:textColor="@color/black"
                android:textSize="13sp" />

            <androidx.cardview.widget.CardView
                style="@style/card"
                android:layout_marginTop="4dp">

                <EditText
                    android:id="@+id/editTextText4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:background="@drawable/etstyle"
                    android:backgroundTint="@color/e1b"
                    android:ems="10"
                    android:inputType="text"
                    android:padding="10dp" />

            </androidx.cardview.widget.CardView>


            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="@string/fulln"
                android:textColor="@color/black"

                android:textSize="13sp" />

            <androidx.cardview.widget.CardView
                style="@style/card"
                android:layout_marginTop="4dp">

                <EditText
                    android:id="@+id/editTextText5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/etstyle"

                    android:backgroundTint="@color/e1b"
                    android:ems="10"
                    android:inputType="text"
                    android:maxLength="10"
                    android:padding="10dp" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/textView10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="@string/mainph"
                android:textColor="@color/black"


                android:textSize="13sp" />

            <androidx.cardview.widget.CardView
                style="@style/card"
                android:layout_marginTop="4dp">


                <EditText
                    android:id="@+id/editTextText6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/etstyle"
                    android:backgroundTint="@color/e1b"
                    android:ems="10"
                    android:inputType="number"
                    android:maxLength="10"
                    android:padding="10dp" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/textView14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="@string/secph"
                android:textColor="@color/black"

                android:textSize="13sp" />

            <androidx.cardview.widget.CardView
                style="@style/card"
                android:layout_marginTop="4dp">


                <EditText
                    android:id="@+id/editTextText10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:background="@drawable/etstyle"
                    android:backgroundTint="@color/e1b"
                    android:ems="10"
                    android:inputType="number"
                    android:padding="10dp" />
            </androidx.cardview.widget.CardView>


            <TextView
                android:id="@+id/textView15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="@string/pas"
                android:textColor="@color/black"

                android:textSize="13sp" />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <androidx.cardview.widget.CardView
                style="@style/card"
                android:layout_marginTop="4dp">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <EditText
                        android:id="@+id/editTextText11"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"

                        android:background="@drawable/etstyle"
                        android:backgroundTint="@color/e1b"
                        android:ems="10"
                        android:inputType="textPassword"
                        android:padding="10dp" />

                    <ImageView
                        android:id="@+id/eyeee"

                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_alignParentEnd="true"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="10dp"
                        android:backgroundTint="@color/e1b"
                        android:src="@drawable/eye_show">

                    </ImageView>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/textView16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="@string/cp"
                android:textColor="@color/black"

                android:textSize="13sp" />

            <androidx.cardview.widget.CardView
                style="@style/card"
                android:layout_marginTop="4dp">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <EditText
                        android:id="@+id/editTextText12"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"


                        android:background="@drawable/etstyle"
                        android:backgroundTint="@color/e1b"
                        android:ems="10"
                        android:inputType="textPassword"
                        android:padding="10dp" />

                    <ImageView
                        android:id="@+id/eyeee1"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_alignParentEnd="true"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="10dp"
                        android:backgroundTint="@color/e1b"
                        android:src="@drawable/eye_show">

                    </ImageView>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>


            <TextView
                android:id="@+id/textView12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="Location"

                android:textColor="@color/black"

                android:textSize="13sp" />

            <androidx.cardview.widget.CardView
                style="@style/card"
                android:layout_marginTop="4dp">

                <Spinner
                    android:id="@+id/spinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:padding="10dp"

                    />

            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/textView13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="@string/company_logo"

                android:textColor="@color/black"

                android:textSize="13sp" />

            <RelativeLayout
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_gravity="center">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/profile_image"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:src="@drawable/icon_profile" />

                <ImageView
                    android:id="@+id/image"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginEnd="6dp"
                    android:layout_marginBottom="6dp"
                    android:src="@drawable/ic_camera" />


            </RelativeLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="15dp"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/checkBox"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:buttonTint="@color/yellow">

                </CheckBox>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="@string/ck"
                    android:textColor="@color/black">

                </TextView>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/checkBox2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:buttonTint="@color/yellow" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginBottom="5dp"
                    android:text="@string/hyperlink"
                    android:textColor="@color/black"

                    >

                </TextView>

            </LinearLayout>

            <Button
                android:id="@+id/button2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:layout_marginTop="12dp"

                android:background="@drawable/b1"
                android:backgroundTint="@color/yellow"
                android:gravity="center"
                android:padding="16dp"
                android:text="@string/a3"
                android:textColor="@color/black" />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="25dp"
                    android:gravity="center"
                    android:text="@string/el"
                    android:textColor="@color/black" />

                <TextView
                    android:id="@+id/textViewlog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="25dp"
                    android:layout_toEndOf="@id/textView18"
                    android:text="@string/el2">

                </TextView>
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>


</androidx.core.widget.NestedScrollView>

